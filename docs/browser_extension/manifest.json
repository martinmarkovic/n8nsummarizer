{
  "manifest_version": 3,
  "name": "YouTube PO Token Extractor",
  "version": "1.0.0",
  "description": "Extracts YouTube PO Token for n8n Summarizer HD quality downloads",
  "permissions": [
    "cookies",
    "tabs",
    "activeTab",
    "clipboardWrite"
  ],
  "host_permissions": [
    "*://*.youtube.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}