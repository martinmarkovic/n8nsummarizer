# n8n Webhook Configuration
# Copy this file to .env and update with your webhook URL

N8N_WEBHOOK_URL=http://localhost:5678/webhook-test/hook1
N8N_TIMEOUT=120

# Application Theme
# Options: light, dark
APP_THEME=light

# Export Preferences
# These settings are saved automatically after first "Send to n8n"
# Defaults: use_original=true, auto_txt=false, auto_docx=false

EXPORT_USE_ORIGINAL_LOCATION=true
EXPORT_AUTO_TXT=false
EXPORT_AUTO_DOCX=false
